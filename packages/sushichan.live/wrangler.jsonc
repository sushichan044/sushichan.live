{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "sushichan-live",
  "compatibility_date": "2025-04-17",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./dist",
    "not_found_handling": "404-page"
  },
  "routes": [
    {
      "custom_domain": true,
      "pattern": "sushichan.live"
    },
    {
      "custom_domain": true,
      "pattern": "www.sushichan.live"
    }
  ],
  "observability": {
    "enabled": true
  }
}
